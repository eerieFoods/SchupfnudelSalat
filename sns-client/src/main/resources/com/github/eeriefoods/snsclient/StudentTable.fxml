<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="studentTable" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.eeriefoods.snsclient.controller.StudentController">
        <children>
            <fx:include source="ToolBar.fxml" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
            <TableView fx:id="TCS_View" editable="true" layoutY="36.0" prefHeight="364.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="36.0">
                <columns>
                    <TableColumn fx:id="TCS_ID" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="ID" />
                    <TableColumn fx:id="TCS_FirstName" maxWidth="400.0" minWidth="100.0" prefWidth="100.0" text="First Name">
                        <cellValueFactory>
                            <PropertyValueFactory property="firstName" />
                        </cellValueFactory>
                        <cellFactory>
                            <TextFieldTableCell fx:factory="forTableColumn" />
                        </cellFactory>
                    </TableColumn>
                    <TableColumn fx:id="TCS_LastName" maxWidth="400.0" minWidth="100.0" prefWidth="100.0" text="Last Name">
                        <cellValueFactory>
                            <PropertyValueFactory property="lastName" />
                        </cellValueFactory>
                        <cellFactory>
                            <TextFieldTableCell fx:factory="forTableColumn" />
                        </cellFactory>
                    </TableColumn>
                    <TableColumn fx:id="TCS_JavaLevel" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="Java Level">
                        <cellValueFactory>
                            <PropertyValueFactory property="javaLevel" />
                        </cellValueFactory>
                        <cellFactory>
                            <TextFieldTableCell fx:factory="forTableColumn" />
                        </cellFactory>
                    </TableColumn>
                    <TableColumn fx:id="TCS_Course" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="Course ID">
                        <cellValueFactory>
                            <PropertyValueFactory property="courseId" />
                        </cellValueFactory>
                        <cellFactory>
                            <TextFieldTableCell fx:factory="forTableColumn" />
                        </cellFactory>
                    </TableColumn>
                    <TableColumn fx:id="TCS_Company" maxWidth="400.0" minWidth="100.0" prefWidth="100.0" text="Company">
                        <cellValueFactory>
                            <PropertyValueFactory property="company" />
                        </cellValueFactory>
                        <cellFactory>
                            <TextFieldTableCell fx:factory="forTableColumn" />
                        </cellFactory>
                    </TableColumn>
                </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
            </TableView>
        </children>
</AnchorPane>
